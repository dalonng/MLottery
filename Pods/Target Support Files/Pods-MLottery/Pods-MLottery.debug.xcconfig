FRAMEWORK_SEARCH_PATHS = "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_FRAMEWORK_BUILD_PATH/CCHexagonFlowLayout.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/MLPSpotlight.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/MagicalRecord.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/Masonry.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/pop.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "CCHexagonFlowLayout" -framework "MLPSpotlight" -framework "MagicalRecord" -framework "Masonry" -framework "pop"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-MLottery
PODS_ROOT = ${SRCROOT}/Pods